{
  "name": "electron-updater",
  "productName": "electron-updater",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build:win-ia32": "electron-builder build --win --ia32",
    "build:win": "electron-builder build --win"
  },
  "keywords": [],
  "author": {
    "name": "박수용",
    "email": "dfort123@nate.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_updater"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "12.0.2",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "productName": "my-new-app",
    "asar": true,
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "shortcutName": "Builder 테스트 프로그램",
      "artifactName": "Builder 테스트 프로그램 ${version}.${ext}",
      "uninstallDisplayName": "Builder 테스트 프로그램"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qkrtndyd12/electron-updater.git"
  }
}
